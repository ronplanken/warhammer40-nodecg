{
  "name": "react-vite-tmpl",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:schema-types": "onchange schemas configschema.json -i -- npm run schema-types",
    "dev:tsc": "tsc -b src/browser src/extension -w --preserveWatchOutput",
    "dev:vite": "vite",
    "dev:nodecg": "onchange extension schema configschema.json -ik -- node --enable-source-maps ../..",
    "format": "npm run test:prettier -- --write",
    "build": "vite build",
    "schema-types": "del-cli src/nodecg/generated && nodecg schema-types -o src/nodecg/generated",
    "test": "run-s schema-types test:*",
    "test:prettier": "prettier \"**/*.{ts,tsx,json,html,yml,css,scss,md}\" --check",
    "test:lint": "eslint --ext .ts,.tsx .",
    "test:types": "tsc -b src/browser src/extension",
    "prepare": "simple-git-hooks || true"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,json,html,yml,css,scss,md}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@googleapis/sheets": "^4.0.2",
    "app-root-path": "^3.1.0",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "got": "^12.6.0",
    "klona": "^2.0.6",
    "moment": "^2.29.4",
    "react-transition-group": "^4.4.5",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@types/app-root-path": "^1.2.5",
    "@types/node": "^18.16.4",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "@types/react-highlight-words": "^0.16.4",
    "@types/rimraf": "^3.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "del-cli": "^5.0.0",
    "delay": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globby": "^13.1.4",
    "gsap": "^3.11.5",
    "lint-staged": "^13.2.2",
    "modern-normalize": "^1.1.0",
    "nodecg-cli": "^8.6.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "pixi.js": "^6.5.9",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "simple-git-hooks": "^2.8.1",
    "sort-package-json": "^2.4.1",
    "styled-components": "^5.3.10",
    "ts-nodecg": "^0.3.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.9"
  },
  "nodecg": {
    "compatibleRange": "^2.0.0",
    "dashboardPanels": [
      {
        "name": "exammple",
        "title": "example",
        "file": "example.html",
        "width": 4,
        "headerColor": "#00BEBE",
        "workspace": "Example"
      }
    ],
    "graphics": [
      {
        "file": "example.html",
        "width": 1920,
        "height": 1030
      }
    ],
    "assetCategories": [],
    "mount": []
  }
}